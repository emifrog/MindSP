  "dependencies": {
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@prisma/client": "^5.20.0",
    "next-auth": "^5.0.0-beta.22",
    "bcryptjs": "^2.4.3",
    "zod": "^3.23.8",
    "react-hook-form": "^7.53.0",
    "@hookform/resolvers": "^3.9.0",
    "zustand": "^4.5.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "redis": "^4.7.0",
    "bullmq": "^5.13.2",
    "@radix-ui/react-dialog": "^1.1.1",
